{
  "name": "information-manipulation-analyzer-processor",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "Martin Ratinaud <martinratinaud@gmail.com>",
  "license": "MIT",
  "private": false,
  "prettier": "prettier-config-ambanum",
  "scripts": {
    "build": "tsc src/**/*",
    "start": "NODE_PATH=src ts-node src/index.ts",
    "dev": "NODE_PATH=src node --trace-warnings -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "docker:build": "docker build -t information-manipulation-analyzer-processor .",
    "docker:run": "docker rm information-manipulation-analyzer-processor && docker run -e DATABASE_URL=mongodb://host.docker.internal:27017/splitter-dev --name information-manipulation-analyzer-processor -it information-manipulation-analyzer-processor",
    "docker:ssh": "docker exec -it information-manipulation-analyzer-processor /bin/bash"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "mongoose": "^5.11.19",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.32",
    "prettier": "^2.2.1",
    "prettier-config-ambanum": "^1.0.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0"
  }
}
